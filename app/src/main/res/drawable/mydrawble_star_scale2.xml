<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:width="80dp"
        android:height="15dp">
        <shape xmlns:android="http://schemas.android.com/apk/res/android"
            android:shape="rectangle">
            <!-- 填充颜色 -->
            <solid android:color="#f65922" />
            <!-- 线的宽度,虚线宽度，虚线间隔 -->
            <stroke
                android:width="0.4dp"
                android:color="#000000" />
            <!-- 矩形的圆角半径,半圆形，设置大一点就好了 -->
            <corners android:radius="100dp" />
        </shape>
    </item>
    <item
        android:top="2dp"
        android:left="5dp"
        android:width="10dp"
        android:height="10dp"
        android:drawable="@drawable/mydrawble_star_filled">
    </item>
    <item
        android:top="2dp"
        android:left="20dp"
        android:width="10dp"
        android:height="10dp"
        android:drawable="@drawable/mydrawble_star_filled">
    </item>
    <item
        android:top="2dp"
        android:left="35dp"
        android:width="10dp"
        android:height="10dp"
        android:drawable="@drawable/mydrawble_star_empty">
    </item>
    <item
        android:top="2dp"
        android:left="50dp"
        android:width="10dp"
        android:height="10dp"
        android:drawable="@drawable/mydrawble_star_empty">
    </item>
    <item
        android:top="2dp"
        android:left="65dp"
        android:width="10dp"
        android:height="10dp"
        android:drawable="@drawable/mydrawble_star_empty">
    </item>

</layer-list>